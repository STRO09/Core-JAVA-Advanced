package messing;

import java.util.Scanner;

public class Main {
	public static int givePrimeSum(int m,int n) {
		int sum = 0,count=2,flag=0;
		if(m+n==2) {
			 sum=5;
		}

		for(int i=4;i<10000;i++) {
			for(int j=2;j<i;j++) {
				if(i%j==0) {
					flag=1;
				}
				if(j==i-1) {
					if(flag==0) {
						count+=1;
					}
				}
			}
			if(count>=m && count<=m+n && flag==0) {
				sum+=i;
			}
			if(count==m+n) {
				return sum;
			}
			flag=0;
		}
		
		return sum;
		
	}
	
public static void main(String[] args) {

	Scanner scanner = new Scanner(System.in);
	System.out.println("Enter m:");
	int m =scanner.nextInt();
	System.out.println("Enter n:");
	int n =scanner.nextInt();
	
	int sum = givePrimeSum(m,n);
	System.out.println(sum);
}
}
